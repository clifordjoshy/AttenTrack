<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid
                android:color="#97f283"/>
            <size
                android:width="100px"
                android:height="100px"/>
        </shape>
    </item>
    <item
        android:top="30px"
        android:bottom="30px"
        android:left="30px"
        android:right="30px"
        android:drawable="@drawable/plus_icon"
        android:gravity="center">
    </item>
</layer-list>