<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:layout_height="match_parent"
            android:shape="oval">
            <solid android:color="#ff6255" />
            <size
                android:width="100px"
                android:height="100px" />
        </shape>
    </item>
    <item
        android:top="46px"
        android:bottom="46px"
        android:left="30px"
        android:right="30px">
        <shape android:shape="rectangle">
            <solid android:color="#000000"/>
            <size
                android:height="8px"
                android:width="40px"/>
            <corners
                android:radius="10px"/>
        </shape>
    </item>
</layer-list>